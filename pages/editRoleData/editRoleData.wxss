.to-register{
    width: 90%;
    padding: 8rpx 5%;
    text-align: end;
    background-color: #fadab7;

    color: #e44c11;
}
.form-box {
    padding: 5rpx 3%;
    width: 94%;
    border-top: 8rpx solid #f5f5f5;
}
.form-box .item {
    display: flex;
    justify-content: space-between;
    padding: 20rpx 0;
    width: 100%;
    font-size: 28rpx;
    color: #010101;
    letter-spacing: 2rpx;
    border-top: 1rpx solid #f5f5f5;
    align-items: center;
}

.form-box .item input{
    min-width: 120rpx;
    flex: 1;
}
.form-box .item label{
    min-width: 135rpx;
    color: #333333;
}
.phone {
  margin-left:15rpx; 
}

