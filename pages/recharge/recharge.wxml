<view class='head head-col'>

  <view class='reconciliation' bindtap='goPage'>充值</view>
  <view class='title'>账户余额(元)</view>
  <view class='money'>{{czAmt||0}}</view>
</view>

<view class='list'>
  <view class='li' wx:for="{{data}}" wx:key="index">
    <view class='item' >
      <text class='type'>{{item.memo || '取消大苏打'}}</text>
      <text class='right'>{{item.createDate || '2020-02-02 15:2:2'}}</text>
    </view>
    <view class='item' >
      <text class='sheetNo'>{{item.subOrderNo || 'ZC56432181156564'}}</text>
      <text class='right money'>{{item.busiFrozenAmt || '-13156'}}</text>
    </view>
  </view>
</view>
<text  class="page-bottom-null" wx:if="{{true}}"></text>
