.mark{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #00000050;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 10;
}

.title {
  text-align: center;
  font-size: 35rpx;
  font-weight: bold;

}

.print-dialog {
  height: 650rpx;
  width: 500rpx;
  padding: 30rpx;
  background-color: #fff;
  border-radius: 20rpx;
  position: relative;
}

.btn {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 90rpx;
  line-height: 90rpx;
  color: rgb(137, 190, 84);
  font-weight: bold;
  font-size: 38rpx;
  text-align: center;
  border-top: 1rpx solid #eee;
}

.list {
  width: 100%;
  height: 550rpx;
  overflow-y: scroll;
}

.item {
  font-size: 30rpx;
  padding: 24rpx 34rpx;
  margin-top: 25rpx;
  color: #333333;
  border: 1rpx solid #cccccc;
  border-radius: 15rpx;
}

.act {
  border: 1rpx solid #e43d3d;
}